{
  "name": "ddt-project-manager",
  "version": "1.0.0",
  "description": "Dutch Domino Team Project Manager",
  "main": "index.js",
  "repository": "git@github.com:saphewilliam/ddt-project-manager.git",
  "author": "William Ford <william@saphewebdesign.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build:wasm:debug": "asc --target debug",
    "build:wasm:release": "asc --target release",
    "build:wasm": "rimraf public/wasm && yarn build:wasm:debug && yarn build:wasm:release",
    "build:site": "next build",
    "build": "yarn build:wasm && yarn build:site",
    "start": "next start",
    "lint": "next lint --fix && tsc",
    "generate:prisma": "prisma generate",
    "generate:nexus": "yarn tsn --files src/graphql/schema",
    "generate:graphql": "graphql-codegen -r dotenv/config -c .codegenrc.yml",
    "generate": "yarn generate:prisma && yarn generate:nexus && yarn generate:graphql",
    "tsn": "ts-node -r tsconfig-paths/register -O \"{\\\"module\\\":\\\"commonjs\\\"}\""
  },
  "prisma": {
    "seed": "yarn tsn src/graphql/seed.ts"
  },
  "dependencies": {
    "@dicebear/avatars": "4.9.0",
    "@dicebear/avatars-jdenticon-sprites": "4.9.0",
    "@heroicons/react": "1.0.4",
    "@prisma/client": "3.0.1",
    "apollo-server-micro": "2.25.2",
    "as-bind": "0.8.2",
    "bcrypt": "5.0.1",
    "clsx": "1.1.1",
    "graphql": "15.5.2",
    "graphql-request": "3.5.0",
    "graphql-tag": "2.12.5",
    "next": "11.1.2",
    "nexus": "1.1.0",
    "nexus-prisma": "0.33.0",
    "react": "17.0.2",
    "react-cookie": "4.1.1",
    "react-dom": "17.0.2",
    "react-hot-toast": "2.1.1",
    "react-loading": "2.0.3",
    "sass": "1.39.0",
    "sharp": "0.29.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.1.1",
    "@graphql-codegen/typescript": "2.2.0",
    "@graphql-codegen/typescript-graphql-request": "4.1.3",
    "@graphql-codegen/typescript-operations": "2.1.3",
    "@tailwindcss/forms": "0.3.3",
    "@tailwindcss/typography": "0.4.1",
    "@types/bcrypt": "5.0.0",
    "@types/node": "16.7.10",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "assemblyscript": "0.19.12",
    "autoprefixer": "10.3.4",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "graphql-codegen-plugin-typescript-swr": "0.6.1",
    "postcss": "8.3.6",
    "prettier": "2.3.2",
    "prisma": "3.0.1",
    "tailwind-scrollbar": "1.3.1",
    "tailwindcss": "2.2.9",
    "ts-node": "10.2.1",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.2"
  }
}
